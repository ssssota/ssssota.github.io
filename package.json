{
  "name": "ssssota.github.io",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prefetch": "pnpm --dir graphql-type codegen && tsx --tsconfig tsconfig.tsx.json -r dotenv/config scripts/prefetch.ts",
    "dev": "vite dev",
    "build": "pnpm prefetch && svelte-kit sync && vite build",
    "preview": "vite preview",
    "check": "pnpm prefetch && svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "fmt": "prettier --plugin-search-dir . --write ."
  },
  "dependencies": {
    "@octokit/core": "^4.1.0",
    "graphql": "^16.8.1",
    "svelte-exmarkdown": "^1.1.6",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@iarna/toml": "^2.2.5",
    "@rollup/pluginutils": "^5.0.2",
    "@sveltejs/adapter-static": "^1.0.0",
    "@sveltejs/kit": "^1.15.2",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.8.1",
    "svelte": "^3.54.0",
    "svelte-check": "^3.0.1",
    "tslib": "^2.4.1",
    "tsx": "^3.12.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.5",
    "graphql-tag": "^2.12.6"
  },
  "type": "module",
  "packageManager": "pnpm@8.6.12"
}
