{
  "name": "ssssota.github.io",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prefetch": "pnpm --dir graphql-type codegen && tsx --tsconfig tsconfig.tsx.json -r dotenv/config scripts/prefetch.ts",
    "dev": "vite dev",
    "build": "pnpm prefetch && svelte-kit sync && vite build",
    "preview": "vite preview",
    "check": "pnpm prefetch && svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "fmt": "prettier --plugin-search-dir . --write ."
  },
  "dependencies": {
    "@octokit/core": "^4.2.4",
    "graphql": "^16.8.1",
    "svelte-exmarkdown": "^1.1.8",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@iarna/toml": "^2.2.5",
    "@rollup/pluginutils": "^5.1.0",
    "@sveltejs/adapter-static": "^1.0.6",
    "@sveltejs/kit": "^1.30.3",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "graphql-tag": "^2.12.6",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "svelte": "^3.59.2",
    "svelte-check": "^3.6.2",
    "tslib": "^2.6.2",
    "tsx": "^3.14.0",
    "typescript": "^4.9.5",
    "vite": "^4.5.1"
  },
  "type": "module",
  "packageManager": "pnpm@8.12.1"
}
