{
  "name": "ssssota.github.io",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prefetch": "pnpm --dir graphql-type codegen && tsx --tsconfig tsconfig.tsx.json -r dotenv/config scripts/prefetch.ts",
    "dev": "vite dev",
    "build": "pnpm prefetch && svelte-kit sync && vite build",
    "preview": "vite preview",
    "check": "pnpm prefetch && svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "lint": "prettier --check . && eslint .",
    "fmt": "prettier --write ."
  },
  "dependencies": {
    "@octokit/core": "^4.2.4",
    "budoux": "^0.6.1",
    "graphql": "^16.8.1",
    "svelte-exmarkdown": "^3.0.1",
    "svg2png-wasm": "^1.4.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@iarna/toml": "^2.2.5",
    "@rollup/pluginutils": "^5.1.0",
    "@sveltejs/adapter-static": "^3.0.0",
    "@sveltejs/kit": "^2.0.2",
    "@sveltejs/vite-plugin-svelte": "^3.0.1",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.35.1",
    "graphql-tag": "^2.12.6",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "svelte": "^4.2.8",
    "svelte-check": "^3.6.2",
    "tslib": "^2.6.2",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.12"
  },
  "type": "module",
  "packageManager": "pnpm@8.12.1"
}
